@media screen and (min-width:760px) and (max-width:1250px){
	.intro-container{padding: 3% 7% 0% 7%;}
	.intro-container h1{font-size: 50px;} 
	.intro-container h2{font-size: 35px;} 
	.intro-container img{
		width: 225px;
		height: 225px;
		padding-right: 10px;
	}
	.header{
		padding-left: 0px;
	}


	.intro-container .btn{
		width: 125px;
		height: 125px;
		font-size: 16px;
	}

	.intro-container .button-placement{
		float: right;
		padding-right: 50px;
	}
	.header1{
		font-size: 50px;
	}


