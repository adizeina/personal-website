	.intro-container h1{font-size: 40px;} 
	.intro-container h2{font-size: 25px;}
	.intro-container{
		padding-left: 
	}
	.intro-container img{
		width: 180px;
		height: 180px;
		padding-right: 10px;
	}

	.intro-container .btn{
		width: 100px;
		height: 100px;
		font-size: 13px;
	}
	.porfolio-container{padding: 3% 7% 0% 7%;}
	

	.project-container .top-container {
		display:none;
	
	}
	.project-container .boxed{
